---------------------- 자료 게시판  ----------------------
import java.util.Date;

public class File_Board {

	private int seq;
	private String title;
	private String content;
	private String id;
	private Date regdate;
	private int readcount;
	private int report;
	private String filename;
	private String newfilename;
	
	public File_Board() {
		
	}
	
	public File_Board(int seq, String title, String content, String id, Date regdate, int readcount, int report,
			String filename, String newfilename) {
		super();
		this.seq = seq;
		this.title = title;
		this.content = content;
		this.id = id;
		this.regdate = regdate;
		this.readcount = readcount;
		this.report = report;
		this.filename = filename;
		this.newfilename = newfilename;
	}

	public File_Board(int seq, String title, String id, Date regdate, int readcount, int report) {
		super();
		this.seq = seq;
		this.title = title;
		this.id = id;
		this.regdate = regdate;
		this.readcount = readcount;
		this.report = report;
	}

	public File_Board(String title, String content, String id, String filename, String newfilename) {
		super();
		this.title = title;
		this.content = content;
		this.id = id;
		this.filename = filename;
		this.newfilename = newfilename;
	}
	
	import java.util.Date;

public class Request_Board {
	
	private int seq;
	private int refer;
	private int step;
	private String title;
	private String content;
	private String id;
	private Date regdate;
	private int readcount;
	private String answered;
	
  
  ---------------------- 1:1 건의 게시판  ----------------------
	public Request_Board() {
		
	}

	public Request_Board(int seq, int refer, int step, String title, String content, String id, Date regdate,
			int readcount, String answered) {
		super();
		this.seq = seq;
		this.refer = refer;
		this.step = step;
		this.title = title;
		this.content = content;
		this.id = id;
		this.regdate = regdate;
		this.readcount = readcount;
		this.answered = answered;
	}

	public Request_Board(int seq, String title, String id, Date regdate, int readcount, String answered) {
		super();
		this.seq = seq;
		this.title = title;
		this.id = id;
		this.regdate = regdate;
		this.readcount = readcount;
		this.answered = answered;
	}
	
	

}


	
	
	
	
}
